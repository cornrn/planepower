include_rules

# MHE Settings, formerly stored in mhe_settings.m
W0=0
Ncvp=10
Tc=1
r=1.2

# Run the code generator
: ../mhe_codegen |> ../mhe_codegen $(W0) $(Ncvp) $(Tc) 1 $(r); |> acado.h auxiliary_functions.c condensing.c gauss_newton_method.c integrator.c solver.cpp solver.hpp

# Compile the generated code
CPPFLAGS_ACADO += -I..
CPPFLAGS_ACADO += -w
: foreach condensing.c gauss_newton_method.c integrator.c | acado.h solver.hpp |> !cc_acado |>
: solver.cpp | solver.hpp |> !cxx_acado |>
