ifdef CAROUSEL2
ifdef PC

include_rules

CPPFLAGS_OROCOS +=-I../../siemensDrives

HEADER=../../siemensDrives/SiemensDriveState.h
CLASS=SiemensDriveState

: $(HEADER) |> !codegen_orocos_typekit |>
: $(CLASS)Typekit.cpp |> !cxx_orocos |>

: $(HEADER) |> !codegen_proto |>
: $(CLASS)Telemetry.proto |> !protoc |>
: $(CLASS)Telemetry.pb.cc | $(CLASS)Telemetry.pb.h |> !cxx_orocos |>

: $(HEADER) |> !codegen_protobuf_bridge |>

CXXFLAGS_OROCOS+=-Wno-unused-function
: $(CLASS)Telemetry.cpp | $(CLASS)Telemetry.pb.h |> !cxx_orocos |>
CXXFLAGS_OROCOS+=-Wunused-function

LDFLAGS_OROCOS += -lprotobuf -lzmq
: *.o |> !ld_orocos |>

.gitignore

endif
endif
